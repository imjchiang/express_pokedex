<div class="container">
    <h1 class="text-center">Your Favorite Pokemon</h1>
    <p class="text-center">
      <a href="/pokemon">Favorites</a>
    </p>
  
    <% pokemon.forEach(function(poke) { %>
        <div class="well">
          <h2><%= poke.name %></h2>
          <form method="GET" action="/pokemon/<%= poke.name.toLowerCase() %>">
            <button class="btn btn-primary" type="submit">Details</button>
          </form>
        </div>
    <% }); %>
  </div>
  